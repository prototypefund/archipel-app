{
  "name": "archipel-app",
  "productName": "Archipel",
  "version": "2.0.1",
  "license": "GPL-3.0",
  "description": "Archipel Desktop App",
  "author": "Archipel:Somoco Team",
  "scripts": {
    "setup": "npm install && cd app; npm install && cd ../electron; npm install && cd ../server; npm install",
    "server": "cd server; ARCHIPEL_STATIC_PATH=../app/dist/web npm run start",
    "electron": "cd electron; NODE_ENV=development npm run start",
    "watch": "cd app; npm run watch",
    "dev": "NODE_ENV=development npm-run-all -p watch server electron",
    "lint": "standard"
  },
  "standard": {
    "ignore": []
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "opn": "^5.3.0",
    "standard": "^11.0.1",
    "tape": "^4.9.1"
  },
  "dependencies": {
    "application-config-path": "^0.1.0",
    "depj": "^1.0.3",
    "param": "^1.1.0"
  }
}
